{
    "manifest_version": 3,
    "name": "Navigator",
    "version": "1.0",
    "description": "Click to navigate to a specific URL",
    "permissions": [
        "tabs",
        "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "",
      "default_icon": {
        "16": "icon16.png"
      }
    },
    "icons": {
      "16": "icon16.png"
    }
}